<!DOCTYPE html>
<html lang="en">
 
<head>
    <%- include('partes/head') %>
</head>
 
<body class="container">
 
    <header>
        <%- include('partes/header') %>
    </header>
 
    <main>
       <div id="main-content" class="container">
            <div class="section">
                <h1 class="header center">Login</h1>
            
                <div class="row">
                    <div class="col s12 center">
                        <h3><i class="mdi-content-send brown-text"></i></h3>
                        <p class="center-align light">Introduzca su email para realizar login</p>
                        <table class="login">
                            <tr>
                                <td>Email</td>
                                <td><input type="text" name="email" id="email" /></td>
                            </tr>
                            <tr>
                                <td colspan="2" class="tdLogin"><input type="button" name="enviar" class="botonLogin" id="enviar" value="LOGIN" onClick="realizarLogin();"/></td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </main>
 
    <footer>
        <%- include('partes/footer') %>
    </footer>
 
</body>
 
</html>
